    "graphql": "0.13.2",
    "graphql-middleware": "1.3.3",
    "prettier": "1.12.1",
    "prettier-check": "2.0.0",
    "rimraf": "2.6.2",
    "semantic-release": "15.6.6",
    "tslint": "5.10.0",
    "tslint-config-prettier": "1.14.0",
    "tslint-config-standard": "7.0.0",
    "typescript": "2.8.4"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/maticzav/graphql-middleware-sentry.git"
